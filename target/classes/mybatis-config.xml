<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 카멜표기법으로 바꾸기 위한 설정 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.example.board.model.MemberModel" alias="MemberModel" />
		<typeAlias type="com.example.board.model.BbsModel" alias="BbsModel" />
	</typeAliases>

</configuration>